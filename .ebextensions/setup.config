commands:
  01_migrate:
    leader_only: true
    command: cd /var/app/current/server && npx sequelize-cli db:migrate --env $(/opt/elasticbeanstalk/bin/get-config environment | jq -r '.NODE_ENV')
  02_setvars:
      command: /opt/elasticbeanstalk/bin/get-config environment | jq -r 'to_entries | .[] | "export \(.key)=\"\(.value)\""' > /etc/profile.d/sh.local
packages:
    yum:
        jq: []