commands:
  01_migrate:
    leader_only: true
    command: "export NODE_ENV=staging && cd /var/app/current/ && npx sequelize-cli db:migrate"
  02_setvars:
        command: /opt/elasticbeanstalk/bin/get-config environment | jq -r 'to_entries | .[] | "export \(.key)=\"\(.value)\""' > /etc/profile.d/sh.local
packages:
    yum:
        jq: []